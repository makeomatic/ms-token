{
  "name": "ms-token",
  "version": "2.0.0",
  "description": "utils for makeomatic mservice for encoding/decoding various token types and storing associated metadata with them",
  "main": "./lib/index.js",
  "scripts": {
    "test": "yarn lint && yarn test-unit && yarn test-integration",
    "test-unit": "mocha test/unit.js",
    "test-integration": "mdep test run -t ./test/integration.js",
    "lint": "eslint ./src",
    "compile": "babel ./src -d ./lib --copy-files",
    "prepublishOnly": "yarn compile",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/ms-token.git"
  },
  "keywords": [
    "crypto",
    "tokens",
    "challenge",
    "utils",
    "mservice"
  ],
  "author": "Vitaly Aminev <v@makeomatic.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-token/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-token#readme",
  "peerDependencies": {
    "ioredis": "4.x.x"
  },
  "engine": {
    "node": ">= 12.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-strict-mode": "^7.8.3",
    "@babel/register": "^7.9.0",
    "@makeomatic/deploy": "^10.1.4",
    "babel-plugin-istanbul": "^6.0.0",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-makeomatic": "^5.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "ioredis": "^4.16.3",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "base64-url": "^2.3.3",
    "chance": "^1.1.4",
    "get-value": "^3.0.1",
    "glob": "^7.1.6",
    "is": "^3.3.0",
    "lodash.compact": "^3.0.1",
    "lodash.omit": "^4.5.0",
    "uuid": "^8.0.0"
  },
  "files": [
    "src/",
    "lib/",
    "bin/"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $HUSKY_GIT_PARAMS"
    }
  }
}
