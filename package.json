{
  "name": "ms-token",
  "version": "2.0.0",
  "description": "utils for makeomatic mservice for encoding/decoding various token types and storing associated metadata with them",
  "main": "./lib/index.js",
  "scripts": {
    "test": "yarn lint && yarn test-unit && yarn test-integration",
    "test-unit": "mocha test/unit.js",
    "test-integration": "mdep test run -t ./test/integration.js",
    "lint": "eslint ./src",
    "compile": "babel ./src -d ./lib --copy-files",
    "prepublishOnly": "yarn compile",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makeomatic/ms-token.git"
  },
  "keywords": [
    "crypto",
    "tokens",
    "challenge",
    "utils",
    "mservice"
  ],
  "author": "Vitaly Aminev <v@makeomatic.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-token/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-token#readme",
  "peerDependencies": {
    "ioredis": "3.x.x || 4.x.x"
  },
  "engine": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@makeomatic/deploy": "^8.2.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-makeomatic": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-promise": "^4.0.1",
    "ioredis": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "base64-url": "^2.2.0",
    "chance": "^1.0.16",
    "glob": "^7.1.3",
    "is": "^3.2.1",
    "joi": "^14.0.6",
    "lodash": "^4.17.11",
    "uuid": "^3.3.2"
  },
  "files": [
    "src/",
    "lib/",
    "bin/"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $HUSKY_GIT_PARAMS"
    }
  }
}
